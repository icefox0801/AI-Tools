# AI-Tools Environment Variables
# Copy this file to .env and fill in your values

# Tavily API Key for web search (get free key at https://tavily.com)
# Reserved for future use
TAVILY_API_KEY=your_tavily_api_key_here

# Serper API Key for web search (get free key at https://serper.dev)
# Reserved for future use
SERPER_API_KEY=your_serper_api_key_here

# Optional: Override default Ollama model
# OLLAMA_MODEL=qwen3:14b

# Optional: Disable text refinement (punctuation/correction)
# ENABLE_TEXT_REFINER=false
# ENABLE_CORRECTION=false
